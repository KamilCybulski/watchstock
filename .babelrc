{ 
  "presets": [ "react", ["env", { "modules": false }] ],
  "plugins": [ "react-hot-loader/babel",  
    "babel-plugin-transform-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "presets": [["env"], "react"],
      "plugins": [
        "babel-plugin-transform-object-rest-spread",
        "transform-class-properties"
      ]
    }
  }
}